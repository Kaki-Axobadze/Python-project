    "import PyPDF2\n",
    "import sys\n",
    "import os\n",
    "merger = PyPDF2.PdfMerger()\n",
    "for file in os.listdir(os.curdir):\n",
    "    if file.endswith(\".pdf\"):\n",
    "        merger.append(file)\n",
    "    merger.write(\"შეერთებული_ფაილი.pdf\")\n"
   ]
